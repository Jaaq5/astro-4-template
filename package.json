{
  "name": "astro-4-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "format": "pnpm exec prettier --write \"src/**/*.{js,jsx,ts,tsx,md,json,astro}\" --config .prettierrc.mjs",
    "lint": "pnpm exec eslint \"src/**/*.{js,jsx,ts,tsx,astro}\" --fix",
    "lint:css": "pnpm exec stylelint  \"src/**/*.{css,tsx,astro}\"",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "astro": "^4.16.2",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.6.0",
    "astro-eslint-parser": "^1.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.10.0",
    "stylelint-config-html": "^1.1.0"
  }
}
